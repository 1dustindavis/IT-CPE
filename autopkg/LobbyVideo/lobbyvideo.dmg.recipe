    <key>Description</key>
    <string>Pacakges the lobbyvideo file</string>
    <key>Identifier</key>
    <string>com.facebook.autopkg.dmg.lobbyvideo</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>lobbyvideo</string>
        <key>RULESET_NAME</key>
        <string>lobbyvideo.mp4</string>
        <key>SOURCE_DIR</key>
        <string>/Users/Shared/lobby_video</string>
        <key>DESTINATION_PATH</key>
        <string>/Users/svc-lobby/Desktop</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.0</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>PkgRootCreator</string>
            <key>Arguments</key>
            <dict>
                <key>pkgroot</key>
                <string>%RECIPE_CACHE_DIR%/</string>
                <key>pkgdirs</key>
                <dict>
                    <key>dmg_root</key>
                    <string>0775</string>
                </dict>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>Copier</string>
            <key>Arguments</key>
            <dict>
               <key>source_path</key>
               <string>%SOURCE_DIR%/%RULESET_NAME%</string>
               <key>destination_path</key>
               <string>%RECIPE_CACHE_DIR%/dmg_root/</string>
            </dict>
        </dict>
        <dict>
                <key>Processor</key>
                <string>DateVersioner</string>
                <key>Arguments</key>
                <dict>
                    <key>notime</key>
                    <false/>
                </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>DmgCreator</string>
            <key>Arguments</key>
            <dict>
                <key>dmg_root</key>
                <string>%RECIPE_CACHE_DIR%/dmg_root/</string>
                <key>dmg_path</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%-%version%.dmg</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
